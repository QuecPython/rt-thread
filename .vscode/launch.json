{
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceFolder}/bsp/stm32/encortec/application/build/encortec-application.elf",
            "name": "Debug with ST-Link",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "stlink",
            "interface": "swd",
            "device": "STM32U545RE",
            "svdFile": "${workspaceRoot}/.vscode/STM32U545.svd",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            },
            "runToEntryPoint": "main",
            "showDevDebugOutput": "raw"
        }
    ]
}
